	<RecipeDef ParentName="RecipeBionicProsthesis">
		<defName>CreateBionicEye</defName>
		<label>Craft bionic eye</label>
		<description>Crafts a bionic eye from plasteel.</description>
		<jobString>Crafting bionic.</jobString>
		<ingredients>
			<li>
				<filter>
					<thingDefs>
						<li>Plasteel</li>
					</thingDefs>
				</filter>
				<count>100</count>
			</li>
			<li>
				<filter>
					<thingDefs>
						<li>Component</li>
					</thingDefs>
				</filter>
				<count>5</count>
			</li>
		</ingredients>
		<fixedIngredientFilter>
			<thingDefs>
				<li>Plasteel</li>
				<li>Component</li>
			</thingDefs>
		</fixedIngredientFilter>
		<products>
			<BionicEye>1</BionicEye>
		</products>
	</RecipeDef>

