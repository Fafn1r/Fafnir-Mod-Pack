	</RecipeDef>

	<RecipeDef ParentName="RecipeSyntheticOrgan">
		<defName>CreateJoywire</defName>
		<label>Craft joywire</label>
		<description>Creates a joywire. Needs gold and plasteel for high quality circuitry.</description>
		<jobString>Creating implant.</jobString>
		<ingredients>
			<li>
				<filter>
					<thingDefs>
						<li>Gold</li>
					</thingDefs>
				</filter>
				<count>5</count>
			</li>
			<li>
				<filter>
					<thingDefs>
						<li>Plasteel</li>
					</thingDefs>
				</filter>
				<count>60</count>
			</li>
		</ingredients>
		<fixedIngredientFilter>
			<thingDefs>
				<li>Gold</li>
				<li>Plasteel</li>
			</thingDefs>
		</fixedIngredientFilter>
		<products>
			<Joywire>1</Joywire>
		</products>
		<researchPrerequisite>Neuroscience</researchPrerequisite>
	</RecipeDef>

