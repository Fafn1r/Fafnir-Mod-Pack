	<RecipeDef ParentName="RecipeAdvancedProsthesis">
		<defName>CreateAdvancedBionicEye</defName>
		<label>Upgrade bionic eye</label>
		<description>Upgrades a bionic eye.</description>
		<jobString>Upgrading bionic.</jobString>
		<ingredients>
			<li>
				<filter>
					<thingDefs>
						<li>BionicEye</li>
					</thingDefs>
				</filter>
				<count>1</count>
			</li>
      <li>
				<filter>
					<thingDefs>
						<li>Hyperweave</li>
					</thingDefs>
				</filter>
				<count>2</count>
			</li>
			<li>
				<filter>
					<thingDefs>
						<li>Plasteel</li>
					</thingDefs>
				</filter>
				<count>80</count>
			</li>
		</ingredients>
		<fixedIngredientFilter>
			<thingDefs>
				<li>BionicEye</li>
        <li>Hyperweave</li>
				<li>Plasteel</li>
			</thingDefs>
		</fixedIngredientFilter>
		<products>
			<AdvancedBionicEye>1</AdvancedBionicEye>
		</products>
	</RecipeDef>

