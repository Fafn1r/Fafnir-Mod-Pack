<RecipeDef ParentName="SurgeryBone">
  <defName>RepairClavicle</defName>
  <label>repair clavicle</label>
  <description>Repair clavicle</description>
  <workerClass>Recipe_InstallNaturalBodyPart</workerClass>
  <jobString>Repairing clavicle</jobString>
  <surgerySuccessChanceFactor>1.3</surgerySuccessChanceFactor>
        <workAmount>4000</workAmount>
  <ingredients>
    <li>
      <filter>
        <categories>
          <li>Medicine</li>
        </categories>
      </filter>
      <count>2</count>
    </li>
    <li>
      <filter>
        <thingDefs>
          <li>Plasteel</li>
        </thingDefs>
      </filter>
      <count>10</count>
    </li>
  </ingredients>
  <fixedIngredientFilter>
    <categories>
      <li>Medicine</li>
    </categories>
    <exceptedThingDefs>
      <li>HerbalMedicine</li>
    </exceptedThingDefs>
    <thingDefs>
      <li>Plasteel</li>
    </thingDefs>
  </fixedIngredientFilter>
  <appliedOnFixedBodyParts>
    <li>LeftClavicle</li>
    <li>RightClavicle</li>
  </appliedOnFixedBodyParts>
      <researchPrerequisite>SimpleProsthetics</researchPrerequisite>
</RecipeDef>

