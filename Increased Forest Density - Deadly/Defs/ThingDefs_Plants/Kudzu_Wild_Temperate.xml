<?xml version="1.0" encoding="utf-8" ?>
<Defs>

  <!--===========================  Wild - clustering bushes (some harvestable) ==============================-->


  <ThingDef ParentName="PlantBase">
    <defName>PlantRaspberry</defName>
    <label>raspberry bush</label>
    <description>A bushy plant which yields delicious red berries.</description>
    <statBases>
      <MaxHitPoints>120</MaxHitPoints>
    </statBases>
    <graphicData>
      <texPath>Things/Plant/Raspberry</texPath>
      <graphicClass>Graphic_Random</graphicClass>
      <shadowData>
        <volume>(0.6, 0.2, 0.6)</volume>
      </shadowData>
    </graphicData>
    <selectable>true</selectable>
    <fillPercent>0.25</fillPercent>
    <pathCost>35</pathCost>
    <ingestible>
      <nutrition>0.25</nutrition>
    </ingestible>
    <plant>
			<fertilityMin>0.5</fertilityMin>
			<fertilitySensitivity>0.5</fertilitySensitivity>
			<growDays>3</growDays>
			<leaflessGraphicPath>Things/Plant/Bush_Leafless</leaflessGraphicPath>
			<harvestYield>30</harvestYield>
			<harvestWork>800</harvestWork>
			<harvestTag>Standard</harvestTag>
			<harvestDestroys>false</harvestDestroys>
			<harvestedThingDef>RawBerries</harvestedThingDef>
			<wildCommonalityMaxFraction>3.0</wildCommonalityMaxFraction>
			<wildClusterSizeRange>
				<min>2</min>
				<max>5</max>
			</wildClusterSizeRange>
			<wildClusterRadius>4</wildClusterRadius>
			<visualSizeRange>
				<min>0.6</min>
				<max>1.5</max>
			</visualSizeRange>
      <topWindExposure>0.3</topWindExposure>
    </plant>
  </ThingDef>


  <ThingDef ParentName="PlantBase">
    <defName>PlantBush</defName>
    <label>bush</label>
    <description>A short shrub. Does not typically talk.</description>
    <statBases>
      <MaxHitPoints>120</MaxHitPoints>
    </statBases>
    <graphicData>
      <texPath>Things/Plant/Bush</texPath>
      <graphicClass>Graphic_Random</graphicClass>
       <shadowData>
         <volume>(0.37, 0.2, 0.37)</volume>
       </shadowData>
    </graphicData>
    <selectable>true</selectable>
    <fillPercent>0.25</fillPercent>
    <pathCost>32</pathCost>
    <ingestible>
      <nutrition>0.17</nutrition>
    </ingestible>
    <plant>
			<fertilityMin>0.4</fertilityMin>
			<fertilitySensitivity>0.5</fertilitySensitivity>
			<growDays>3</growDays>
			<leaflessGraphicPath>Things/Plant/Bush_Leafless</leaflessGraphicPath>										
			<wildClusterSizeRange>
				<min>2</min>
				<max>5</max>
			</wildClusterSizeRange>
			<wildClusterRadius>4</wildClusterRadius>
			<visualSizeRange>
				<min>0.4</min>
				<max>1.8</max>
			</visualSizeRange>
      <topWindExposure>0.3</topWindExposure>
    </plant>
  </ThingDef>



  <!--======================= Wild plants - carpets ========================-->

  <ThingDef ParentName="PlantBase">
    <defName>PlantGrass</defName>
    <label>grass</label>
    <description>Wild grass. Grows anywhere there is a little light and minimally fertile ground.</description>
    <statBases>
      <MaxHitPoints>85</MaxHitPoints>
      <Beauty>2</Beauty>
    </statBases>
    <graphicData>
      <texPath>Things/Plant/Grass</texPath>
      <graphicClass>Graphic_Random</graphicClass>
    </graphicData>
    <hideAtSnowDepth>0.5</hideAtSnowDepth>
    <ingestible>
      <nutrition>0.15</nutrition>
    </ingestible>
    <plant>
			<fertilityMin>0.05</fertilityMin>
			<fertilitySensitivity>0.3</fertilitySensitivity>
			<growDays>2.5</growDays>
			<leaflessGraphicPath>Things/Plant/Grass_Leafless</leaflessGraphicPath>
      <harvestWork>40</harvestWork>
      <fertilityMin>0.05</fertilityMin>
      <maxMeshCount>9</maxMeshCount>
      <visualSizeRange>
        <min>0.4</min>
        <max>0.8</max>
      </visualSizeRange>
      <topWindExposure>0.4</topWindExposure>
      <reproduceMtbDays>3</reproduceMtbDays>
    </plant>
  </ThingDef>


  <ThingDef ParentName="PlantBase">
    <defName>PlantDandelion</defName>
    <label>dandelions</label>
    <description>A common small flower. Though it is often considered a weed, it is pleasant to look at.</description>
    <statBases>
      <MaxHitPoints>85</MaxHitPoints>
      <Beauty>5</Beauty>
    </statBases>
    <graphicData>
      <texPath>Things/Plant/Dandelion</texPath>
      <graphicClass>Graphic_Random</graphicClass>
    </graphicData>
    <hideAtSnowDepth>0.5</hideAtSnowDepth>
    <ingestible>
      <nutrition>0.14</nutrition>
    </ingestible>
    <plant>
			<fertilitySensitivity>0.0</fertilitySensitivity>
			<growDays>2.5</growDays>
			<sowTags>
        <li>Ground</li>
      </sowTags>
      <fertilityMin>0.05</fertilityMin>
      <maxMeshCount>25</maxMeshCount>
      <visualSizeRange>
        <min>0.3</min>
        <max>0.6</max>
      </visualSizeRange>
      <topWindExposure>0.3</topWindExposure>
	  <reproduceMtbDays>3</reproduceMtbDays>
      <wildClusterSizeRange>
        <min>2</min>
        <max>6</max>
      </wildClusterSizeRange>
      <wildClusterRadius>4</wildClusterRadius>
    </plant>
  </ThingDef>

  <ThingDef ParentName="PlantBase">
    <defName>PlantAstragalus</defName>
    <label>astragalus</label>
    <description>A small perennial wildflower that grows in alpine climates.</description>
    <statBases>
      <MaxHitPoints>85</MaxHitPoints>
      <Beauty>5</Beauty>
    </statBases>
    <graphicData>
      <texPath>Things/Plant/Astragalus</texPath>
      <graphicClass>Graphic_Random</graphicClass>
    </graphicData>
    <hideAtSnowDepth>0.5</hideAtSnowDepth>
    <ingestible>
      <nutrition>0.15</nutrition>
    </ingestible>
    <plant>
      <fertilityMin>0.05</fertilityMin>
			<fertilitySensitivity>0</fertilitySensitivity>
			<growDays>2.5</growDays>
      <maxMeshCount>25</maxMeshCount>
      <visualSizeRange>
        <min>0.3</min>
        <max>0.7</max>
      </visualSizeRange>
      <topWindExposure>0.3</topWindExposure>
      <reproduceMtbDays>5</reproduceMtbDays>
      <wildClusterSizeRange>
        <min>2</min>
        <max>6</max>
      </wildClusterSizeRange>
      <wildClusterRadius>4</wildClusterRadius>
    </plant>
  </ThingDef>

  <!--============================ Wild - Trees ============================-->

  <ThingDef ParentName="TreeBase">
    <defName>PlantTreeOak</defName>
    <label>oak tree</label>
    <description>A strong hardwood tree. Slow-going, but its wood is very strong.</description>
    <graphicData>
      <texPath>Things/Plant/TreeOak</texPath>
      <graphicClass>Graphic_Random</graphicClass>
    </graphicData>
    <statBases>
      <Beauty>5</Beauty>
    </statBases>
    <plant>
      <leaflessGraphicPath>Things/Plant/TreeOak_Leafless</leaflessGraphicPath>
      <visualSizeRange>
        <min>1.0</min>
        <max>4.0</max>
      </visualSizeRange>
	  <growDays>32</growDays>
      <harvestWork>1000</harvestWork>
      <harvestYield>40</harvestYield>
    </plant>
  </ThingDef>

  <ThingDef ParentName="TreeBase">
    <defName>PlantTreePoplar</defName>
    <label>poplar tree</label>
    <description>A softwood tree. Grows fast, but yield little useful wood.</description>
    <graphicData>
      <texPath>Things/Plant/TreePoplar</texPath>
      <graphicClass>Graphic_Random</graphicClass>
    </graphicData>
    <statBases>
      <Beauty>3</Beauty>
    </statBases>
    <ingestible>
      <nutrition>0.75</nutrition>
    </ingestible>
    <plant>
      <leaflessGraphicPath>Things/Plant/TreePoplar_Leafless</leaflessGraphicPath>
      <visualSizeRange>
        <min>1.0</min>
        <max>4.1</max>
      </visualSizeRange>
	  <growDays>18</growDays>
      <harvestWork>180</harvestWork>
      <harvestYield>9</harvestYield>
    </plant>
  </ThingDef>

  <ThingDef ParentName="TreeBase">
    <defName>PlantTreePine</defName>
    <label>pine tree</label>
    <description>A large conifer.</description>
    <graphicData>
      <texPath>Things/Plant/TreePine</texPath>
      <graphicClass>Graphic_Random</graphicClass>
        <shadowData>
          <volume>(0.2, 0.3, 0.2)</volume>
        </shadowData>
    </graphicData>
    <statBases>
      <Beauty>5</Beauty>
    </statBases>
    <ingestible>
      <nutrition>1</nutrition>
    </ingestible>
	<plant>
      <visualSizeRange>
        <min>1.0</min>
        <max>3.9</max>
      </visualSizeRange>
      <growDays>26</growDays>
      <harvestWork>700</harvestWork>
      <harvestYield>30</harvestYield>
    </plant>
  </ThingDef>

  <ThingDef ParentName="TreeBase">
    <defName>PlantTreeBirch</defName>
    <label>birch tree</label>
    <description>A northern tree known for its thin, paper-like bark.</description>
    <graphicData>
      <texPath>Things/Plant/TreeBirch</texPath>
      <graphicClass>Graphic_Random</graphicClass>
      <shadowData>
        <volume>(0.15, 0.3, 0.1)</volume>
      </shadowData>
    </graphicData>
    <statBases>
      <Beauty>4</Beauty>
    </statBases>
    <ingestible>
      <nutrition>1</nutrition>
    </ingestible><plant>
      <leaflessGraphicPath>Things/Plant/TreeBirch_Leafless</leaflessGraphicPath>
      <visualSizeRange>
        <min>1.0</min>
        <max>3.4</max>
      </visualSizeRange>
	  <growDays>16</growDays>
      <harvestWork>160</harvestWork>
      <harvestYield>8</harvestYield>
      <leaflessGraphicPath>Things/Plant/TreeBirch_Leafless</leaflessGraphicPath>
    </plant>
  </ThingDef>

</Defs>