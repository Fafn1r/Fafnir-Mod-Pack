<?xml version="1.0" encoding="utf-8"?>
<Defs>
  <RecipeDef ParentName="ExtractSeed">
    <defName>ExtractSeedPotato</defName>
    <label>extract potato seeds</label>
    <description>Extract seeds from Potatoes.</description>
    <ingredients>
      <li>
        <filter>
          <thingDefs>
            <li>RawPotatoes</li>
          </thingDefs>
        </filter>
        <count>25</count>
      </li>
    </ingredients>
    <fixedIngredientFilter>
      <thingDefs>
        <li>RawPotatoes</li>
      </thingDefs>
    </fixedIngredientFilter>
    <products>
      <SeedPotato>5</SeedPotato>
    </products>
  </RecipeDef>
  
