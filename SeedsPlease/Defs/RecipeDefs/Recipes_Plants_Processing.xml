<?xml version="1.0" encoding="utf-8"?>
<Defs>
  <RecipeDef ParentName="ExtractSeed">
    <defName>ExtractSeedCotton</defName>
    <label>extract cotton seeds</label>
    <description>Extract seeds from Raw Cotton.</description>
    <ingredients>
      <li>
        <filter>
          <thingDefs>
            <li>RawCotton</li>
          </thingDefs>
        </filter>
        <count>25</count>
      </li>
    </ingredients>
    <fixedIngredientFilter>
      <thingDefs>
        <li>RawCotton</li>
      </thingDefs>
    </fixedIngredientFilter>
    <products>
      <SeedCotton>5</SeedCotton>
    </products>
  </RecipeDef>
  
  <RecipeDef ParentName="ProcessRaw">
    <defName>ProcessRawCotton</defName>
    <label>weave cotton cloth</label>
    <description>Weave Raw Cotton into Cloth.</description>
    <ingredients>
      <li>
        <filter>
          <thingDefs>
            <li>RawCotton</li>
          </thingDefs>
        </filter>
        <count>25</count>
      </li>
    </ingredients>
    <fixedIngredientFilter>
      <thingDefs>
        <li>RawCotton</li>
      </thingDefs>
    </fixedIngredientFilter>
    <products>
      <Cloth>25</Cloth>
    </products>
    <skillRequirements>
         <li>
            <skill>Crafting</skill>
            <minLevel>4</minLevel>
         </li>
    </skillRequirements>
  </RecipeDef>
  
  <RecipeDef ParentName="ExtractSeed">
    <defName>ExtractSeedDevilstrand</defName>
