	<RecipeDef Name="RecipeCrateBase" Abstract="True">
		<description>Take all the alcohol bottles out of their storage crate.</description>
		<jobString>Emptying crate.</jobString>
		<workSpeedStat>BrewingSpeed</workSpeedStat>
		<workAmount>50</workAmount>
		<effectWorking>Cook</effectWorking>
		<soundWorking>Recipe_Brewing</soundWorking>
		<recipeUsers>
			<li>Brewery</li>
			<li>VG_UnpackingSpot</li>
		</recipeUsers>
		<workSkill>Cooking</workSkill>
	</RecipeDef>

	<RecipeDef ParentName="RecipeCrateBase">
		<defName>EmptyCrate_cider</defName>
		<label>empty cider crate</label>
		<ingredients>
			<li>
				<filter>
					<thingDefs>
						<li>cider_Crate</li>
					</thingDefs>
				</filter>
				<count>1</count>
			</li>
		</ingredients>
		<fixedIngredientFilter>
			<thingDefs>
				<li>cider_Crate</li>
			</thingDefs>
		</fixedIngredientFilter>
		<products>
			<cider>75</cider>
		</products>
	</RecipeDef>

