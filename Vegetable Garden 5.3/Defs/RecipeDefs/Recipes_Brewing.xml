	</fixedIngredientFilter>
	<products>
	  <Chocomilk>4</Chocomilk>
	</products>
	<recipeUsers>
         <li>Brewery</li>
      </recipeUsers>
	<workSkill>Cooking</workSkill>
</RecipeDef>

