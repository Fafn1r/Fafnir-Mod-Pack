<?xml version="1.0" encoding="utf-8" ?>
<RecipeDefs>


  <RecipeDef>
    <defName>ExtractRawSteelLeaf</defName>
    <label>smelt metal from Steel Shrooms</label>
    <description>Use heat and strong electromagnets to separate useful metal from Steel Shrooms. \n\nMakes 4 steel from 16 shrooms.</description>
    <jobString>Smelting metal from plants.</jobString>
    <workAmount>1200</workAmount>
    <workSpeedStat>SmeltingSpeed</workSpeedStat>
    <effectWorking>Smelt</effectWorking>
    <soundWorking>Recipe_Smelt</soundWorking>
    <recipeUsers>
       <li>ElectricSmelter</li>
    </recipeUsers>
    <ingredients>
			<li>
				<filter>
					<thingDefs>
						<li>RawSteelLeaf</li>
					</thingDefs>
				</filter>
				<count>16</count>
			</li>
		</ingredients>
		<fixedIngredientFilter>
			<thingDefs>
				<li>RawSteelLeaf</li>
			</thingDefs>
		</fixedIngredientFilter>
		<products>
			<Steel>4</Steel>
		</products>
		<skillRequirements>
			<li>
				<skill>Crafting</skill>
				<minLevel>8</minLevel>
			</li>
		</skillRequirements>
		<workSkill>Crafting</workSkill>
	</RecipeDef>

