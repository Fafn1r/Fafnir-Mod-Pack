<?xml version="1.0" encoding="utf-8" ?>
<Defs>

  <!--===========================  Flowers Vanilla ==============================-->
  <ThingDef ParentName="VG_PlantBase">
    <defName>PlantDandelion</defName>
    <label>dandelions</label>
    <description>A common small flower. Though it is often considered a weed, it is pleasant to look at.</description>
    <statBases>
      <MaxHitPoints>85</MaxHitPoints>
      <Beauty>5</Beauty>
    </statBases>
    <graphicData>
      <texPath>Things/Plant/Dandelion</texPath>
      <graphicClass>Graphic_Random</graphicClass>
    </graphicData>
    <hideAtSnowDepth>0.5</hideAtSnowDepth>
    <ingestible>
      <nutrition>0.14</nutrition>
    </ingestible>
    <plant>
      <fertilityMin>0.05</fertilityMin>
      <maxMeshCount>25</maxMeshCount>
      <visualSizeRange>
        <min>0.3</min>
        <max>0.4</max>
      </visualSizeRange>
      <growDays>2.5</growDays>
      <topWindExposure>0.3</topWindExposure>
      <reproduceMtbDays>3.5</reproduceMtbDays>
      <fertilitySensitivity>1</fertilitySensitivity>
        <sowTags>
        <li>Ground</li>
      </sowTags>
      <wildClusterSizeRange>
        <min>2</min>
        <max>6</max>
      </wildClusterSizeRange>
      <wildClusterRadius>4</wildClusterRadius>
    </plant>
  </ThingDef>



  <ThingDef ParentName="VG_PlantBase">
    <defName>PlantRose</defName>
    <label>rose</label>
    <description>A beautiful cultivated flower, symbolic of love and romance since ancient times.</description>
    <statBases>
      <MaxHitPoints>85</MaxHitPoints>
      <Beauty>14</Beauty>
    </statBases>
    <graphicData>
      <texPath>Things/Plants/Rose</texPath>
      <graphicClass>Graphic_Random</graphicClass>
    </graphicData>
    <altitudeLayer>Item</altitudeLayer>
    <selectable>true</selectable>
    <ingestible>
      <foodType>Plant</foodType>
      <preferability>NeverForNutrition</preferability>
      <nutrition>0.10</nutrition>
    </ingestible>
    <plant>
      <dieIfLeafless>true</dieIfLeafless>
      <harvestTag>Standard</harvestTag>
      <harvestDestroys>True</harvestDestroys>
      <sowWork>400</sowWork>
      <sowTags>
        <li>Ground</li>
        <li>Decorative</li>
      </sowTags>
      <growMinGlow>0.3</growMinGlow>
      <topWindExposure>0.1</topWindExposure>
      <growDays>1.5</growDays>
      <lifespanFraction>7</lifespanFraction>
      <fertilitySensitivity>1.0</fertilitySensitivity>
      <visualSizeRange>
        <min>0.3</min>
        <max>1.05</max>
      </visualSizeRange>
    </plant>
  </ThingDef>

